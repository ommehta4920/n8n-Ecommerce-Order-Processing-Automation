    {
      "parameters": {
        "jsCode": "const order = $items(\"Set\")[0].json;  // Adjust node name if different\nconst item = $items(\"Inventory Check\")[0].json; // the inventory/stock node output\n\nconst html = `\n<!DOCTYPE html>\n<html>\n<head>\n  <meta charset=\"UTF-8\">\n  <title>Invoice for Order ${order.order_id}</title>\n  <style>\n    body { font-family: Arial, sans-serif; line-height: 1.5; }\n    table { border-collapse: collapse; width: 100%; margin-top: 20px; }\n    th, td { border: 1px solid #000; padding: 8px; text-align: left; }\n    th { background-color: #f2f2f2; }\n    h1 { color: #333; }\n    p { margin: 5px 0; }\n  </style>\n</head>\n<body>\n  <h1>Invoice for Order ${order.order_id}</h1>\n  <p><strong>Customer:</strong> ${order.name} (${order.email})</p>\n  <table>\n    <thead>\n      <tr>\n        <th>SKU</th>\n        <th>Product</th>\n        <th>Qty</th>\n        <th>Unit Price</th>\n        <th>Total</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr>\n        <td>${item.sku}</td>\n        <td>${item.name}</td>\n        <td>${item.qty_ordered}</td>\n        <td>${item.price}</td>\n        <td>${item.qty_ordered * item.price}</td>\n      </tr>\n    </tbody>\n  </table>\n  <p><strong>Grand Total:</strong> ${item.qty_ordered * item.price}</p>\n</body>\n</html>\n`;\n\nreturn [{ json: { html } }];\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
