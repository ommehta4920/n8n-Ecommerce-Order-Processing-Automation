    {
      "parameters": {
        "jsCode": "// Get all inventory rows\nconst inventoryItems = $items(\"Google Sheet\").map(i => i.json); // Google Sheets output\n\n// Get all order items from Set node\nconst orderItems = $items(\"Set\")[0].json.items; // adjust node name if different\n\n// Array to store results\nconst result = [];\n\norderItems.forEach(item => {\n  // Find matching inventory row by SKU\n  const inventory = inventoryItems.find(row => row.sku === item.sku);\n\n  if (inventory) {\n    result.push({\n      json: {\n        sku: item.sku,\n        name: item.name,\n        qty_ordered: item.qty,\n        stock_qty: inventory.stock_qty,\n        price: inventory.price,\n        in_stock: Number(inventory.stock_qty) >= Number(item.qty)\n      }\n    });\n  } else {\n    // SKU not found in inventory\n    result.push({\n      json: {\n        sku: item.sku,\n        name: item.name,\n        qty_ordered: item.qty,\n        stock_qty: 0,\n        price: 0,\n        in_stock: false\n      }\n    });\n  }\n});\n\nreturn result;\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1344,
        -576
      ],
