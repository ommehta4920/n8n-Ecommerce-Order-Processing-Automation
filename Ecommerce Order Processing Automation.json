      "parameters": {
        "jsCode": "const rows = $items(\"Sales Record Fetch\").map(i => i.json);\n\nlet totalOrders = rows.length;\nlet totalRevenue = 0;\nlet totalItems = 0;\n\nrows.forEach(r => {\n  totalRevenue += Number(r.qty_ordered) * Number(r.price);\n  totalItems += Number(r.qty_ordered);\n});\n\nreturn [{\n  json: {\n    date: new Date().toISOString().split(\"T\")[0],\n    totalOrders,\n    totalRevenue,\n    totalItems\n  }\n}];\n"
      },
